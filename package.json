{
  "name": "time-table-manager-desktop",
  "description": "SPM Time Table Manager",
  "version": "0.1.0",
  "homepage": "./",
  "main": "public/main.js",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "@sweetalert/with-react": "^0.1.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "bootstrap": "^4.4.1",
    "concurrently": "^5.2.0",
    "electron-is-dev": "^1.2.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-data-table-component": "^6.11.0",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.2",
    "react-spinners": "^0.9.0",
    "reactstrap": "^8.4.1",
    "styled-components": "^5.1.1",
    "sweetalert": "^2.1.2",
    "sweetalert2": "^9.17.1",
    "uuid": "^7.0.2",
    "wait-on": "^5.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/main.js",
    "electron-dev": "concurrently \"SET BROWSER=none&&npm start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^9.1.1",
    "electron-builder": "^22.7.0",
    "nodemon": "^2.0.2"
  },
  "build": {
    "appId": "com.spmteamalpha.app",
    "productName": "SPM Time Table Manager",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extends": null
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kavindu-Tharaka/time-table-manager-desktop.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kavindu-Tharaka/time-table-manager-desktop/issues"
  }
}
